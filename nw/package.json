{
  "name": "uploader",
  "version": "0.0.1",
  "description": "AAS Uploader",
  "main": "index.html",
  "window": {
    "title": "AAS Uploader",
    "toolbar": false,
    "frame": true,
    "resizable": false,
    "position": "center",
    "width": 800,
    "height": 600
  },
  "repository": {
    "url": "https://github.com/your-name/slush-nodewebkit"
  },
  "scripts": {
    "run": "nwbuild --run ./",
    "win": "nwbuild -p win ./",
    "linux": "nwbuild -p linux32,linux64 ./",
    "osx": "nwbuild -p osx ./"
  },
  "author": {
    "name": "Siyuan Gao",
    "url": "siyuangao@gmail.com"
  },
  "dependencies": {
    "node-webkit-builder": "*"
  },
  "devDependencies": {
    "gulp": "^3.6.2",
    "gulp-shell": "^0.2.5"
  }
}
